                },
                {
                    label: "Description",
                    uri: "de/deepamehta/core/property/Description",
                    data_type: "html",
                    js_renderer_class: "BodyTextRenderer",
                    indexing_mode: "FULLTEXT"
                }
            ]
        }
    ]
